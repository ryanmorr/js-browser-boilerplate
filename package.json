{
  "name": "js-browser-boilerplate",
  "description": "Minimal boilerplate for browser-based JavaScript projects",
  "version": "0.4.0",
  "license": "Unlicense",
  "author": "Ryan Morr",
  "homepage": "https://github.com/ryanmorr/js-browser-boilerplate",
  "repository": "github:ryanmorr/js-browser-boilerplate",
  "bugs": "https://github.com/ryanmorr/js-browser-boilerplate/issues",
  "publishConfig": {
    "access": "public"
  },
  "type": "module",
  "main": "dist/<foo>.cjs.js",
  "module": "dist/<foo>.esm.js",
  "browser": "dist/<foo>.umd.js",
  "scripts": {
    "lint": "eslint src/** test/specs/**",
    "test": "karma start karma.config.cjs",
    "build": "rollup -c"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^24.0.1",
    "@rollup/plugin-node-resolve": "^15.0.1",
    "@rollup/plugin-terser": "^0.4.0",
    "chai": "^4.3.7",
    "eslint": "^8.33.0",
    "karma": "^6.4.1",
    "karma-chai": "^0.1.0",
    "karma-chrome-launcher": "^3.1.1",
    "karma-mocha": "^2.0.1",
    "karma-mocha-reporter": "^2.2.5",
    "karma-rollup-preprocessor": "^7.0.8",
    "karma-sinon": "^1.0.5",
    "mocha": "^10.2.0",
    "rollup": "^3.14.0",
    "sinon": "^15.0.1",
    "terser": "^5.16.3"
  }
}
